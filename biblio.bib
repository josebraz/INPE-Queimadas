	issn = {0034-4257},
	journal = {Remote Sensing of Environment},
	keywords = {Fire, Biomass burning, Burned area, MODIS, MCD64A1},
	pages = {72-85},
	title = {The Collection 6 MODIS burned area mapping algorithm and product},
	url = {https://www.sciencedirect.com/science/article/pii/S0034425718303705},
